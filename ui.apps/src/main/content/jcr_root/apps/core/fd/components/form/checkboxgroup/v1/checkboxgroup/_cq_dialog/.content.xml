<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Adaptive Form CheckBoxGroup field"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.forms.components.checkboxgroup.v1.editor]"
    trackingFeature="core-components:adaptiveform-checkboxgroup:v1">
    <content
        granite:class="cmp-adaptiveform-checkboxgroup__editdialog"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                  jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/tabs"
                  maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Basic"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <placeholder
                                    jcr:primaryType="nt:unstructured"
                                    sling:hideResource="{Boolean}true"/>
                            <type
                                    wrapperClass="cmp-adaptiveform-checkboxgroup__type"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete"
                                    sling:orderBefore="enum"
                                    fieldLabel="Type"
                                    forceSelection="{Boolean}true"
                                    name="./type"
                                    key="String"
                                    value="string[]">
                                <options
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete/list"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <string
                                            jcr:primaryType="nt:unstructured"
                                            text="String"
                                            value="string[]"/>
                                    <integer
                                            jcr:primaryType="nt:unstructured"
                                            text="Integer"
                                            value="number[]"/>
                                </items>
                            </type>
                            <enum
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/authoring/components/granite/form/formsmultifield"
                                    fieldDescription="Enter Enum Values"
                                    fieldLabel="Enum">
                                <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        name="./enum"
                                        required="{Boolean}false"/>
                            </enum>
                            <enumNames
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="fd/af/authoring/components/granite/form/formsmultifield"
                                    fieldDescription="Enter Enum Name Values"
                                    fieldLabel="Enum Names">
                                <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        name="./enumNames"
                                        required="{Boolean}false"/>
                            </enumNames>
                            <required
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    checked="{Boolean}false"
                                    fieldLabel="Required field"
                                    labelledBy="Mandatory"
                                    name="./required"
                                    granite:class="cmp-adaptiveform-base__required"
                                    text="Required field"
                                    value="true">
                            </required>
                            <mandatoryMessage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                    fieldDescription="Error message shown when required field left empty."
                                    granite:class="cmp-adaptiveform-base__mandatorymessage"
                                    fieldLabel="Required Field Message"
                                    name="./mandatoryMessage"/>

                            <!--<enum
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                    name="./enum@TypeHint"
                                    value="String"/>-->
<!--
                            <enumNames
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                    name="./enumNames"
                                    typeHint="String[]"/>
-->
                            <alignment
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                    fieldLabel="Item Alignment"
                                    name="./orientation"
                                    text="Item Alignment">
                                <items jcr:primaryType="nt:unstructured">
                                    <horizontal
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/radio"
                                            checked="true"
                                            text="Horizontal"
                                            value="horizontal"/>
                                    <vertical
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/radio"
                                            text="Vertical"
                                            value="vertical"/>
                                </items>
                            </alignment>
                            <autocomplete
                              wrapperClass="cmp-adaptiveform-checkbox__autofillfieldkeyword"
                              jcr:primaryType="nt:unstructured"
                              sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete"
                              fieldLabel="Autofill Attribute"
                              forceSelection="{Boolean}true"
                              name="./autocomplete"
                              value="off">
                                <options
                                  jcr:primaryType="nt:unstructured"
                                  sling:resourceType="granite/ui/components/coral/foundation/form/autocomplete/list"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <off
                                      jcr:primaryType="nt:unstructured"
                                      text="off"
                                      value="off"/>
                                    <on
                                      jcr:primaryType="nt:unstructured"
                                      text="on"
                                      value="on"/>
                                    <name
                                      jcr:primaryType="nt:unstructured"
                                      text="name"
                                      value="name"/>
                                    <first_x0020_name
                                      jcr:primaryType="nt:unstructured"
                                      text="given-name"
                                      value="given-name"/>
                                    <address
                                      jcr:primaryType="nt:unstructured"
                                      text="street-address"
                                      value="street-address"/>
                                    <zip
                                      jcr:primaryType="nt:unstructured"
                                      text="postal-code"
                                      value="postal-code"/>
                                    <card_x0020_name
                                      jcr:primaryType="nt:unstructured"
                                      text="cc-name"
                                      value="cc-name"/>
                                    <middle_x0020_name
                                      jcr:primaryType="nt:unstructured"
                                      text="additional-name"
                                      value="additional-name"/>
                                    <last_x0020_name
                                      jcr:primaryType="nt:unstructured"
                                      text="family-name"
                                      value="family-name"/>
                                    <address-line1
                                      jcr:primaryType="nt:unstructured"
                                      text="address-line1"
                                      value="address-line1"/>
                                    <address-line2
                                      jcr:primaryType="nt:unstructured"
                                      text="address-line2"
                                      value="address-line2"/>
                                    <address-level1
                                      jcr:primaryType="nt:unstructured"
                                      text="address-level1"
                                      value="address-level1"/>
                                    <address-level2
                                      jcr:primaryType="nt:unstructured"
                                      text="address-level2"
                                      value="address-level2"/>
                                    <country
                                      jcr:primaryType="nt:unstructured"
                                      text="country"
                                      value="country"/>
                                    <cc-name
                                      jcr:primaryType="nt:unstructured"
                                      text="cc-name"
                                      value="cc-name"/>
                                    <cc-number
                                      jcr:primaryType="nt:unstructured"
                                      text="cc-number"
                                      value="cc-number"/>
                                    <cc-csc
                                      jcr:primaryType="nt:unstructured"
                                      text="cc-csc"
                                      value="cc-csc"/>
                                    <cc-exp-month
                                      jcr:primaryType="nt:unstructured"
                                      text="cc-exp-month"
                                      value="cc-exp-month"/>
                                    <cc-exp-year
                                      jcr:primaryType="nt:unstructured"
                                      text="cc-exp-year"
                                      value="cc-exp-year"/>
                                    <cc-exp
                                      jcr:primaryType="nt:unstructured"
                                      text="cc-exp"
                                      value="cc-exp"/>
                                    <cc-type
                                      jcr:primaryType="nt:unstructured"
                                      text="cc-type"
                                      value="cc-type"/>
                                </items>
                            </autocomplete>
                        </items>
                    </basic>
                    <validation
                            jcr:primaryType="nt:unstructured"
                            sling:hideResource="{Boolean}true"/>
                    <patterns
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Patterns"
                            sling:hideResource="{Boolean}true"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                    </patterns>
                    <help
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Help Content"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </help>
                    <accessibility
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Accessibility"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </accessibility>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
