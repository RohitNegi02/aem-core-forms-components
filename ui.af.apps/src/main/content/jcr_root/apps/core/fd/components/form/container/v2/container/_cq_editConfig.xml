<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:EditConfig"
          cq:actions="[edit,delete]"
          cq:dialogMode="floating"
          cq:layout="editbar"
          cq:disableTargeting="{Boolean}true">
    <cq:listeners
        jcr:primaryType="cq:EditListenersConfig"
        afterchildinsert="REFRESH_SELF"
        afterchilddelete="REFRESH_SELF"/>
    <cq:actionConfigs
            jcr:primaryType="nt:unstructured">
        <dor
                jcr:primaryType="nt:unstructured"
                handler="CQ.FormsCoreComponents.editorhooks.openDorDialog"
                order="after CONFIGURE"
                icon="filePDF"
                condition="CQ.FormsCoreComponents.editorhooks.isAutoGenerateDoRConfigured"
                text="Document of Record"/>
        <dorPreview
                jcr:primaryType="nt:unstructured"
                handler="CQ.FormsCoreComponents.editorhooks.initPreviewDoR"
                order="after CONFIGURE"
                icon="printPreview"
                condition="CQ.FormsCoreComponents.editorhooks.isAutoGenerateDoRConfigured"
                text="Generate Preview of Document of Record"/>
        <dorPreviewDisabled
                jcr:primaryType="nt:unstructured"
                handler="function(){}"
                icon="printPreview"
                order="after CONFIGURE"
                disabled="{Boolean}true"
                condition="CQ.FormsCoreComponents.editorhooks.isRdeEnabled"
                text="The Document of Record feature is not available for Rapid Development Environment"/>
    </cq:actionConfigs>
</jcr:root>
